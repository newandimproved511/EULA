let blockedDomains = ["cv.shopee.co.th"];

app.post('/update-blocked-domains', (req, res) => {
    const newDomains = req.body.domains;
    blockedDomains = [...blockedDomains, ...newDomains];
    res.send("Blocked domains updated!");
});

let blockedDomains = ["apis.google.com"];

app.post('/update-blocked-domains', (req, res) => {
    const newDomains = req.body.domains;
    blockedDomains = [...blockedDomains, ...newDomains];
    res.send("Blocked domains updated!");
});